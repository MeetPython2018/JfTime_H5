   method: 'post',\n          url: '/ajax/sign_in',\n          data: this.qs.stringify({\n            userName: this.email,\n            password: this.password\n          }),\n          headers: {\n            'Content-Type': 'application/x-www-form-urlencoded'\n          }\n        }).then(function (response) {\n          if (response.data.sign_in === 200) {\n            store.commit('login_in');\n            store.commit('setUserName', _this.email);\n\n            _this.saveState();\n\n            _this.$router.push('/Home/hotAudio');\n          } else {\n            _this.$notify({\n              title: '警告',\n              message: '请检查你的邮箱和密码是否输入正确！',\n              type: 'warning'\n            });\n          }\n        }).catch(function (error) {\n          console.log(error);\n        });\n      } else {\n        this.$notify({\n          title: '警告',\n          message: '邮箱和密码不能为空！',\n          type: 'warning'\n        });\n      }\n    },\n    // 注册提交逻辑\n    SignUpLogin: function SignUpLogin() {\n      var _this2 = this;\n\n      console.log('SignUpLogin');\n\n      if (this.email && this.password) {\n        fetch(\"/ajax/sign_up?userName=\" + this.email + '&password=' + this.password).then(function (e) {\n          return e.json();\n        }).then(function (e) {\n          if (e[0][\"sign_up\"] === 200) {\n            _this2.type = 0;\n          } else {\n            _this2.$notify({\n              title: '警告',\n              message: '请检查你的邮箱和密码是否输入正确！',\n              type: 'warning'\n            });\n          }\n        });\n      } else {\n        this.$notify({\n          title: '警告',\n          message: '邮箱和密码不能为空！',\n          type: 'warning'\n        });\n      }\n    },\n    // 密码强度检测\n    intensity: function intensity() {\n      var value = this.password;\n      var value_length = value.length;\n      var code_length = 0;\n\n      if (/[0-9]/.test(value)) {\n        code_length++;\n      }\n\n      if (/[a-z]/.test(value)) {\n        code_length++;\n      }\n\n      if (/[A-Z]/.test(value)) {\n        code_length++;\n      } //密码是否包含除数字和字母之外的字符\n\n\n      if (value_length >= 6) {\n        this.pick1 = 1;\n      } else {\n        this.pick1 = 0;\n      }\n\n      if (!/\\s/.test(value) && !/[^a-zA-Z0-9]/.test(value) && code_length > 2) {\n        this.pick2 = 1;\n      } else {\n        this.pick2 = 0;\n      } //安全级别\n\n\n      if (code_length >= 2 && value_length >= 15) {\n        this.value = 7;\n      } else if (code_length >= 2 && value_length >= 10) {\n        this.value = 4;\n      } else if (code_length >= 1 && value_length >= 6) {\n        this.value = 3;\n      } else {\n        this.value = 0;\n      }\n    }\n  }\n};",{"version":3,"sources":["Sign_.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8YA,OAAA,KAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,OADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,IAAA,EAAA,CAHA;AAIA,MAAA,KAAA,EAAA,CAJA;AAKA,MAAA,KAAA,EAAA,CALA;AAMA,MAAA,KAAA,EAAA,CANA;AAOA,MAAA,MAAA,EAAA,CAAA,SAAA,EAAA,SAAA,EAAA,SAAA,CAPA;AAQA,MAAA,WAAA,EAAA,CAAA,gBAAA,EAAA,cAAA,EAAA,cAAA,CARA;AASA,MAAA,GAAA,EAAA,CATA;AAUA,MAAA,aAAA,EAAA,CAVA;AAWA,MAAA,cAAA,EAAA;AAXA,KAAA;AAaA,GAhBA;AAiBA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,QAFA,sBAEA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA;AACA,UAAA,GAAA,GAAA,0DAAA;;AACA,UAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA;AACA,UAAA,KAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,eAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA;AAKA,aAAA,KAAA,GAAA,EAAA;AACA;AACA,KAbA;AAcA;AACA,IAAA,SAfA,uBAeA;AACA,MAAA,cAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA;AAC