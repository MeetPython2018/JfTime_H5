   <ul>\r\n                                  <span>密码强度</span>\r\n                                  <el-rate\r\n                              v-model=\"value\"\r\n                              disabled\r\n                              disabled-void-color=\"#ccc\"\r\n                              :max=\"max\"\r\n                              :low-threshold=\"low_threshold\"\r\n                              :high-threshold=\"high_threshold\"\r\n                              text-color=\"#000\"\r\n                              :colors=\"colors\"\r\n                              score-template=\"{value}\">\r\n                            </el-rate>\r\n                              </ul>\r\n                              <ul>\r\n                                  <li>\r\n                                  <i class=\"iconfont icon-yuanquan\" :class=\" pick1===0 ? 'icon-yuanquan':'icon-duihao'\"></i>\r\n                                  <span>6-16个字符</span>\r\n                              </li>\r\n                                  <li>\r\n                                  <i class=\"iconfont icon-yuanquan\" :class=\" pick2===0 ? 'icon-yuanquan':'icon-duihao'\"></i>\r\n                                  <span>只能包含大小写字母、数字、非空字符</span>\r\n                              </li>\r\n                              </ul>\r\n                          </div>\r\n              </template>\r\n              </div>\r\n            <button class=\"signin-button\" @click.prevent=\"type===0 ? SignInLogin():SignUpLogin()\">Login</button>\r\n            <div class=\"link\">\r\n              <a href=\"#\">Forgot password ?</a> or <router-link to=\"/Home\">Go To Home</router-link>\r\n            </div>\r\n          </div>\r\n        </el-main>\r\n      </div>\r\n      <div id=\"bottom\">\r\n        <el-footer class=\"col-sm-12 col-xs-8\">\r\n          <div class=\"links\">\r\n              <a href=\"https://www.youtube.com\"  target=\"_blank\"><i class=\"iconfont icon-youtobe\"></i></a>\r\n             