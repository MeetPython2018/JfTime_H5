        <i :class=\"index===item.id ? 'el-icon-video-pause':btnStyle\" @click=\"playAudio(item.audio_src,item.audio_name,item.creator,item.id)\"></i>\r\n                        </div>\r\n                        <h5>\r\n                            <span>{{ item.audio_name }}</span>\r\n                            <span>{{ item.upload_date }}</span>\r\n                        </h5>\r\n\r\n                    </el-carousel-item>\r\n                  </el-carousel>\r\n                        <div>\r\n                            <el-button type=\"success\" plain>浏览更多</el-button>\r\n                        </div>\r\n                    </div>\r\n                </el-main>\r\n            </div>\r\n            <div class=\"col-xs-6 col-md-8\" id=\"audio\">\r\n                <div class=\"audio_menu\">\r\n                    <div class=\"audio_img\">\r\n                        <div id=\"player\" class=\"aplayer\"></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    <router-view/>\r\n    </div>\r\n</template>\r\n<style scoped>\r\n    *{\r\n      padding: 0;\r\n      margin: 0;\r\n      font-family: '苹方';\r\n    }\r\n    #wenzhao {\r\n      font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n      -webkit-font-smoothing: antialiased;\r\n      -moz-osx-font-smoothing: grayscale;\r\n      text-align: center;\r\n      padding: 0;\r\n      margin: 0;\r\n    }\r\n    .container-full{\r\n        background: #f5f5f5!important;\r\n    }\r\n    /*内容区*/\r\n    .cons{\r\n        width: 100%;\r\n        height: auto;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        padding-top: 4rem;\r\n    }\r\n    .el-main{\r\n        width: 100%;\r\n        padding: 0!important;\r\n        max-width: 1260px!important;\r\n        background: #f5f5f5!important;\r\n        display: flex;\r\n        justify-content: space-around;\r\n        align-items: center;\r\n    }\r\n    .row{\r\n        width: 89%!important;\r\n    }\r\n    .col-md-12{\r\n        padding: 0!important;\r\n    }\r\n    #cover{\r\n        position: relative;\r\n    }\r\n    #cover .el-image{\r\n        width: 100%;\r\n        opacity: .4!important;\r\n    }\r\n    #cover i{\r\n        position: absolute;\r\n        left: 0;\r\n        right: 0;\r\n        bottom: 0;\r\n        top:0;\r\n        margin: auto auto;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n    .el-carousel__item{\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-around;\r\n        align-items: center;\r\n    }\r\n    .el-carousel__item h4{\r\n        width: 89%;\r\n        font-size: .75rem;\r\n        text-align: left;\r\n    }\r\n    .el-carousel__item h5{\r\n        width: 89%;\r\n        font-size: .8rem;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: flex-start;\r\n    }\r\n    .el-icon-video-play,.el-icon-video-pause{\r\n        position: relative;\r\n        font-size: 5.6rem;\r\n        color: #a06619;\r\n        opacity: .7;\r\n    }\r\n      .el-carousel__item:nth-child(2n) {\r\n        background-color: #99a9bf;\r\n      }\r\n\r\n      .el-carousel__item:nth-child(2n+1) {\r\n        background-color: #d3dce6;\r\n      }\r\n\r\n\r\n</style>\r\n<script>\r\n    import APlayer from 'aplayer'\r\n  export default {\r\n      name: \"Wenzhao\",\r\n      data() {\r\n    return {\r\n        btnStyle:'el-icon-video-play',\r\n        index:0,\r\n        dialogTableVisible1: false,\r\n        dialogTableVisible2: false,\r\n        visible: false,\r\n        active:'',\r\n        creator_img_url:['http://120.77.223.229:100/JfTime/net_img/jfTime.png','http://120.77.223.229:100/WenZhao/net_img/wz.png'],\r\n        logo_src:'http://120.77.223.229:100/JfTime/net_img/JF_logo.png',\r\n        fits: 'fill',\r\n        id:'',\r\n        first_page_data:'',\r\n        data_len:''\r\n    };\r\n  },\r\n    methods:{\r\n        playAudio(audio_src,audio_name,creator,index){\r\n            this.index = index\r\n            this.demo.destroy()\r\n            const ap = new APlayer({\r\n                container: document.getElementById('player'),\r\n                fixed: false,\r\n                    autoplay: true,\r\n                    listFolded: true,\r\n                audio: [\r\n                    {name: audio_name,\r\n                    artist: creator,\r\n                    url: audio_src,\r\n                    cover: '',\r\n                    }\r\n                ]\r\n                })\r\n            ap.on('pause', ()=>{\r\n                this.index = ''\r\n\r\n            });\r\n            ap.on('play', ()=>{\r\n                this.index = index\r\n            });\r\n            console.log(this.index)\r\n            this.demo = ap\r\n        }\r\n    },\r\n      async mounted() {\r\n        const ap = new APlayer({\r\n            container: document.getElementById('player'),\r\n            fixed: false,\r\n            audio: [{\r\n            name: '曲名',\r\n            artist: '创作者',\r\n            url: '',\r\n            cover: 'https://cn-south-17-aplayer-46154810.oss.dogecdn.com/yourname.jpg',\r\n            }]\r\n        })\r\n        this.demo = ap\r\n              // 获取音频\r\n          fetch(\"/ajax/wz/dataAudio\").then(function (e) {\r\n                return e.json()\r\n          }).then((e)=>{\r\n              this.first_page_data = e\r\n              console.log(e)\r\n          }),\r\n              //  获取节目数\r\n          fetch(\"/ajax/wz/dataLength\").then(function (e) {\r\n              return e.json()\r\n          }).then((e)=>{\r\n              this.data_len = e['data_len']\r\n          })\r\n        }\r\n  }\r\n</script>\r\n\r\n"],"sourceRoot":"src/components"}]}